{
	"name": "cra-template-nubelsondev",
	"version": "0.1.0",
	"license": "MIT",
	"author": "Nubelson Fernandes <nubelsondev@hotmail.com",
	"description": "A quick start Create React App template with Styled-components",
	"main": "template.json",
	"keywords": [
		"react",
		"create-react-app",
		"cra-template",
		"template",
		"styled-components",
		"layout",
		"theme"
	],
	"files": [
		"template",
		"template.json"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/nubelsondev/cra-template-nubelsondev",
		"directory": "/"
	},
	"bugs": {
		"url": "https://github.com/nubelsondev/cra-template-nubelsondev/issues"
	},
	"dependencies": {
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-scripts": "3.4.1",
		"styled-components": "^5.1.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"clean-files": "rm -rf ./template/public ./template/src",
		"copy-files": "cp -a ./src/. template/src && cp -a ./public/. template/public",
		"prepublishOnly": "yarn clean-files && yarn copy-files"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
